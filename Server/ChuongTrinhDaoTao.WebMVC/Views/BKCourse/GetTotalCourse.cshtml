@model IEnumerable<ChuongTrinhDaoTao.WebMVC.BussinessModel.CourseTotalBM>
@{
    ViewData["Title"] = "GetTotalCourse";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<div class="col-lg-12 grid-margin bg-white stretch-card">
    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table id="bootstrap-data-table" class="table">
                    <thead>
                        <tr>
                            <th>Major Name</th>
                            <th>Cohort Name</th>
                            <th>Total Course</th>
                            <th>Total CodeCredits</th>
                           
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.MajorName</td>
                                <td>@item.CohortName</td>
                                <td>@item.CourseTotal</td>
                                <td>@item.TotalCredits</td>
 
                            </tr>
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- Scripts -->
